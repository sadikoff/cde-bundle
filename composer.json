{
    "name": "koff/cde-bundle",
    "type": "symfony-bundle",
    "description": "Work with gedmo/doctrine-extensions is simple!",
    "keywords": [
        "symfony2",
        "doctrine2",
        "i18n",
        "internationalization",
        "translation",
        "translatable",
        "form",
        "gedmo"
    ],
    "homepage": "https://koff.pro",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Sadicov",
            "email": "sadikoff@gmail.com"
        }
    ],
    "support": {
        "email": "sadikoff@gmail.com"
    },
    "require": {
        "php": "^7.1",
        "gedmo/doctrine-extensions": "~2.4",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/http-kernel": "^4.0"

    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "symfony/debug": "^4.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Koff\\Bundle\\CdeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Koff\\Bundle\\CdeBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install":"dist",
        "sort-packages": true
    }
}
